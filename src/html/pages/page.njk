{% extends "layout.flex.njk" %}
{% set constrain = "" %}
{% set sidebarleft = "" %}
{% set sidebarright = "" %}
{% set body_class = "l-layout--page" %}

{% block content %}
<div class="content-wrapper">
	<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
		<section class="article-content">
			<h1 class="article-h1">{{post.title}}</h1>
			<div class="article-body">
				{{post.content}}
			</div>
		</section>
	</article>
</div><!-- /content-wrapper -->
{% endblock %}
